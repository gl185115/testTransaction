<?xml version="1.0" encoding="UTF-8" ?>
<barcode>
  <sale>
    <items>
       <item>
        <description>JAN・EAN (2段)</description>
        <id>doubleJanSales</id>
        <type>double</type>
        <symbol>EAN</symbol>
        <format>30[0-9]{11}</format>
        <nextformat>6000[0-9]{4}</nextformat>
      </item>
      <item>
        <description>一段目二段目</description>
        <id>section</id>
        <type>single</type>
        <symbol>EAN</symbol>
        <format>30[0-9]{11}(\\s)6000[0-9]{4}</format>
      </item>
      <item>
        <description>一段JAN13</description>
        <id>singleJanSales</id>
        <type>single</type>
        <symbol>EAN</symbol>
        <format>30[0-9]{11}</format>
      </item>
      <item>
        <description>ギフトカード販売</description>
        <id>giftcardSale32</id>
        <type>single</type>
        <symbol>Code128</symbol>
        <format>1[1|2][0-9]{20}</format>
      </item>
    </items>
  </sale>
  <payment>
    <items>
      <item>
        <description>クーポンQRコード</description>
        <id>QRdiscount</id>
        <type>single</type>
        <symbol>NW7</symbol>
        <format>(?&lt;Front&gt;CPN|cpn){1}(?&lt;ReasonId&gt;\\d{2})(?&lt;BizcardId&gt;\\d{2})(?&lt;TagetValue&gt;\\d{7})((?&lt;Item&gt;1)|(?&lt;Transaction&gt;2)|(?&lt;OrderError&gt;[^(1|2)]{1}))((?&lt;PercentOff&gt;1)|(?&lt;AmountOff&gt;2)|(?&lt;SectionError&gt;[^(1|2)]{1}))(?&lt;Value&gt;\\d{7})(?&lt;Date&gt;\\d{6})(?&lt;CheckDigit&gt;\\d{1})(?&lt;CouponDeletFlag&gt;\\d{1})(?&lt;CouponId&gt;[0-9a-zA-Z]{16})?</format>
      </item>
      <item>
        <description>QR値引割引</description>
        <id>QRdiscount</id>
        <type>single</type>
        <symbol>QR</symbol>
        <format>(?&lt;Start&gt;B|b){1}(?&lt;ReasonId&gt;\\d{2})(?&lt;SeqNo&gt;\\d{3})((?&lt;Item&gt;1)|(?&lt;Transaction&gt;3)|(?&lt;OrderError&gt;[^(1|3)]{1}))((?&lt;PercentOff&gt;1)|(?&lt;AmountOff&gt;2)|(?&lt;SectionError&gt;[^(1|2)]{1}))(?&lt;Value&gt;\\d{7})(?&lt;Date&gt;\\d{6})(?&lt;End&gt;C|c){1}.*</format>
      </item>
      <item>
        <description>ポイントクーポン</description>
        <id>pointCoupon</id>
        <type>single</type>
        <symbol>NW7</symbol>
        <format>(D|d){1}(\\d{20})(D|d){1}</format>
      </item>
    </items>
  </payment>
  <signon>
    <items>
      <item>
        <description>サインイン</description>
        <id>signon</id>
        <type>single</type>
        <symbol>Codabar</symbol>
        <format>(A|a){1}([0-9]{4,7})(B|b){1}</format>
      </item>
    </items>
  </signon>
  <member>
    <items>
      <item>
        <description>ポイントカード番号</description>
        <id>member</id>
        <type>single</type>
        <symbol>Codabar</symbol>
        <format>(1|5){1}(\\d{12})</format>
      </item>
    </items>
  </member>
  <transaction>
    <items>
      <item>
        <description></description>
        <id></id>
        <type>single</type>
        <symbol>ITF</symbol>
        <format>(A|a)(\\d{18})(A|a{1})</format>
        <format>()(\\d{20})()</format>
      </item>
      <item>
        <description></description>
        <id></id>
        <type>single</type>
        <symbol>Codabar</symbol>
        <format>(D|d)(\\d{16})(C|c)</format>
      </item>
      <item>
        <description></description>
        <id></id>
        <type>single</type>
        <symbol>Codabar</symbol>
        <format>(D|d)(\\d{16})(C|c)</format>
      </item>
    </items>
  </transaction>
  <tradeSequenceReceipt>
    <items>
      <item>
        <description></description>
        <id></id>
        <type>single</type>
        <symbol>Codabar</symbol>
        <format>CCBBBBRRRRYYMMDDXXXX</format>
      </item>
    </items>
  </tradeSequenceReceipt>
  <multiForwardRecallCard>
    <items>
      <item>
        <description>マルチサスペンドカードのバーコード形式</description>
        <id>multiForwardRecallCard</id>
        <type>single</type>
        <symbol>Codabar</symbol>
        <format>^[Bb](\d{4})[Aa]$</format>
      </item>
    </items>
  </multiForwardRecallCard>
  <machineReadableTravelDocument>
    <items>
      <item>
        <description>Machine Readable Passport</description>
        <id>passportMRZ</id>
        <type>single</type>
        <symbol>OCR-B</symbol>
        <format>([A-Z])([A-Z0-9&lt;])([A-Z]{3})([A-Z&lt;]{39})([A-Z0-9&lt;]{9})([0-9])([A-Z]{3})([0-9]{6})([0-9])([MF&lt;])([0-9]{6})([0-9])([A-Z0-9&lt;]{14})([0-9])([0-9])</format>
      </item>
    </items>
  </machineReadableTravelDocument>
</barcode>