<?xml version="1.0" encoding="UTF-8" ?>
<Receipt type="Voided">
  <ReceiptPaperWidth>40</ReceiptPaperWidth>
  <ReceiptLanguage>jp</ReceiptLanguage>
  <Form>
  	<!-- 再発行 -->
    <Line size="1" align="center" feedNum="0">
      <Condition property="retryflag">
        <eq value="true" property="">
          <Lable property="recptreissue" leftMargin="0"></Lable>
          <Sign length="1" leftMargin="0" value="\n"></Sign>
          <Lable property="recptnotareceipt" leftMargin="0"></Lable>
          <Sign length="1" leftMargin="0" value="\n"></Sign>
          <Lable property="recptalreadyissued" leftMargin="0"></Lable>
        </eq>
      </Condition>
    </Line>
    <!-- logo -->
    <LineImageBmp name="logo"></LineImageBmp>
    <!-- 領収書 -->
    <Line size="1" align="center" feedNum="1">
      <Lable property="receipt" leftMargin="0"></Lable>
    </Line>
    <!-- 店の資料：店名と電話番号とポスター -->
    <!-- 店名 -->
    <Line size="0" align="left" feedNum="0">
      <Lable property="recptstorenamestr" leftMargin="10"></Lable>
      <Item property="storeName" leftMargin="0"></Item>
    </Line>
    <!-- 電話番号 -->
    <Line size="0" align="left" feedNum="1">
      <Lable property="recpttelno" leftMargin="10"></Lable>
      <Item property="telNo" leftMargin="0"></Item>
    </Line>
    <!-- ポスター -->
    <Line size="0" align="left" feedNum="1">
      <Item property="ads" leftMargin="0"></Item>
    </Line>
    <!-- 担当者： original transaction -->
    <Line size="0" align="left" feedNum="1">
      <Lable property="recptoperator2" leftMargin="0"></Lable>
      <Sign length="1" leftMargin="0" value="："></Sign>
      <Item property="originalOperatorID" leftMargin="0"></Item>
    </Line>
    <!-- train mode -->
    <Line size="0" align="left" feedNum="1">
         <Condition property="trainModeFlag">
          <eq value="True" property="">
           <Sign length="5" leftMargin="0" value="* "></Sign>
           <Sign length="1" leftMargin="0" value="*"></Sign>
           <Lable property="receiptTrainMode" leftMargin="2"></Lable>
           <Sign length="5" leftMargin="2" value="* "></Sign>
           <Sign length="1" leftMargin="0" value="*"></Sign>
          </eq>
         </Condition>
    </Line>
    <!-- 受注取消 -->
    <Line size="0" align="left" feedNum="1">
      <Condition property="language">
        <eq value="en" property="">
          <Sign length="7" leftMargin="0" value="* "></Sign>
          <Sign length="1" leftMargin="0" value="*"></Sign>
          <Lable property="recptadvancevoidstr" leftMargin="1"></Lable>
          <Sign length="7" leftMargin="1" value="* "></Sign>
          <Sign length="1" leftMargin="0" value="*"></Sign>
        </eq>
      </Condition>
      <Condition property="language">
        <ne value="en" property="">
          <Sign length="7" leftMargin="0" value="* "></Sign>
          <Sign length="1" leftMargin="0" value="*"></Sign>
          <Lable property="recptadvancevoidstr" leftMargin="1"></Lable>
          <Sign length="7" leftMargin="1" value="* "></Sign>
          <Sign length="1" leftMargin="0" value="*"></Sign>
        </ne>
      </Condition>
    </Line>
    <!-- 前受金 -->
    <Line size="1" align="left" feedNum="1">
      <Condition property="AdvanceAmount">
        <ne value="0" property="">
          <Lable property="advancePaymentstr" leftMargin="2"></Lable>
          <Price property="AdvanceAmount" isMinus="false" leftMargin="max"></Price>
        </ne>
      </Condition>
    </Line>
    <LineItems>
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherFaceValueAmount">
          <ne value="0" property="">
            <Condition property="voucherTypeCode">
              <eq value="voucher" property="">
                <!-- 金券 -->
                <Lable property="voucherStr" leftMargin="4"></Lable>
                <Price property="voucherFaceValueAmount" isMinus="false"
                  leftMargin="max"></Price>
              </eq>
            </Condition>
            <Condition property="voucherTypeCode">
              <eq value="giftcertificate" property="">
                <!-- 商品券 -->
                <Lable property="gtftCertStr" leftMargin="4"></Lable>
                <Price property="voucherFaceValueAmount" isMinus="false"
                  leftMargin="max"></Price>
              </eq>
            </Condition>
          </ne>
        </Condition>
      </Line>
    </LineItems>
    <!-- 振込 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="bankTransferAmount">
        <ne value="0" property="">
          <Lable property="recptBankTransfer" leftMargin="4"></Lable>
          <Price property="bankTransferAmount" isMinus="false" leftMargin="max"></Price>
        </ne>
      </Condition>
    </Line>
    <!-- 現金 -->
    <Line size="0" align="left" feedNum="1">
      <Condition property="cashPament">
        <ne value="0" property="">
          <Lable property="recptcash" leftMargin="4"></Lable>
          <Price property="cashPament" isMinus="false" leftMargin="max"></Price>
        </ne>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
     <Condition property="HoldVoidType">
       <eq value="">
         <Condition property="CustomrOrderVoidType">
            <eq value="">
              <Condition property="cashPament">
                <eq value="0" property="">
                  <Sign leftMargin="0" length="1" value=" "></Sign>
                </eq>
              </Condition>
            </eq>
         </Condition>
       </eq>
     </Condition>
    </Line>
    <!-- GiftCard -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="giftCardCount">
        <ne value="0" property="">
          <Sign leftMargin="0" length="20" value="・"></Sign>
        </ne>
      </Condition>
    </Line>
    <!-- ぎフトカードご利用 -->
    <Line size="0" align="center" feedNum="0">
      <Condition property="giftCardCount">
        <ne value="0" property="">
          <Lable property="giftCardTitleStr" leftMargin="0"></Lable>
        </ne>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
      <Condition property="giftCardCount">
        <ne value="0" property="">
          <Sign leftMargin="0" length="20" value="・"></Sign>
        </ne>
      </Condition>
    </Line>
    <LineItems>
      <!-- カード番号 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Lable property="giftCardNoStr" leftMargin="0"></Lable>
            <Item property="voucherSerialNumberEncrypt" leftMargin="max"></Item>
          </eq>
        </Condition>
      </Line>
      <!-- ご利用前金額 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Lable property="giftCardPreBalanceStr" leftMargin="0"></Lable>
            <Condition property="language">
              <eq value="en" property="">
                <Price property="voucherFaceValueAmount" isMinus="false"
                  leftMargin="max"></Price>
              </eq>
            </Condition>
            <Condition property="language">
              <ne value="en" property="">
                <Price property="voucherFaceValueAmount" isMinus="false"
                  leftMargin="max" unitFlg="true"></Price>
              </ne>
            </Condition>
          </eq>
        </Condition>
      </Line>
      <!-- ご利用金額 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Lable property="giftCardUsedStr" leftMargin="0"></Lable>
            <Condition property="language">
              <eq value="en" property="">
                <Price property="voucherAmount" isMinus="false"
                  leftMargin="max"></Price>
              </eq>
            </Condition>
            <Condition property="language">
              <ne value="en" property="">
                <Price property="voucherAmount" isMinus="false"
                  leftMargin="max" unitFlg="true"></Price>
              </ne>
            </Condition>
          </eq>
        </Condition>
      </Line>
      <!-- ご利用後金額 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Lable property="giftCardBalanceStr" leftMargin="0"></Lable>
            <Condition property="language">
              <eq value="en" property="">
                <Price property="voucherUnspentAmount" isMinus="false"
                  leftMargin="max"></Price>
              </eq>
            </Condition>
            <Condition property="language">
              <ne value="en" property="">
                <Price property="voucherUnspentAmount" isMinus="false"
                  leftMargin="max" unitFlg="true"></Price>
              </ne>
            </Condition>
          </eq>
        </Condition>
      </Line>
      <!-- 有効期限 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Lable property="giftCardExpiryStr" leftMargin="0"></Lable>
            <Condition property="voucherExpirationDate">
              <ne value="">
                <Date property="voucherExpirationDate" formatFrom="yyyy-MM-dd"
                  formatTo="yyyy/MM/dd" isSysDate="false" leftMargin="max"></Date>
              </ne>
            </Condition>
          </eq>
        </Condition>
      </Line>
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Sign leftMargin="0" length="20" value="・"></Sign>
          </eq>
        </Condition>
      </Line>
    </LineItems>
    <!-- クレジットカード -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Sign leftMargin="0" length="20" value="・"></Sign>
        </ne>
      </Condition>
    </Line>
       <Line size="0" align="center" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <!-- ICクレジットご利用 -->
          <Condition property="creditType">
            <eq value="IC" property="">
              <Lable property="icCreditTitle" leftMargin="0"></Lable>
            </eq>
          </Condition>
          <!-- 中国銀聯ご利用 -->
          <Condition property="creditType">
            <eq value="UnionPay" property="">
              <Lable property="unionpayTitle" leftMargin="0"></Lable>
            </eq>
          </Condition>
          <!-- MSクレジットご利用 -->
          <Condition property="creditType">
            <eq value="Credit" property="">
              <Lable property="recptcredittitle2" leftMargin="0"></Lable>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Sign leftMargin="0" length="20" value="・"></Sign>
        </ne>
      </Condition>
    </Line>
    <!-- カード会社 或いは 金融機関 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <!-- カード会社 -->
          <Condition property="creditType">
            <ne value="UnionPay" property="">
              <Lable property="recptcordcompanyname" leftMargin="0"></Lable>
              <Item property="cardCompany" leftMargin="max"></Item>
            </ne>
          </Condition>
          <!-- 金融機関 -->
          <Condition property="creditType">
            <eq value="UnionPay" property="">
              <Lable property="financingInstitutio" leftMargin="0"></Lable>
              <Item property="cardCompany" leftMargin="max"></Item>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- クレジット番号 或いは カード番号 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <!-- クレジット番号 -->
          <Condition property="creditType">
            <ne value="UnionPay" property="">
              <Lable property="recptcreditno" leftMargin="0"></Lable>
              <Item property="creditNoEncrypt" leftMargin="max-3"></Item>
              <Sign leftMargin="0" length="1" value="/00"></Sign>
            </ne>
          </Condition>
          <!-- カード番号 -->
          <Condition property="creditType">
            <eq value="UnionPay" property="">
              <Lable property="unionpayCardNo" leftMargin="0"></Lable>
              <Item property="creditNoEncrypt" leftMargin="max-3"></Item>
              <Sign leftMargin="0" length="1" value="/00"></Sign>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- 有効期限 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Lable property="recptexpiry" leftMargin="0"></Lable>
          <Sign leftMargin="max" length="1" value="XX/XX"></Sign>
        </ne>
      </Condition>
    </Line>
    <!-- クレジット伝票番号 或いは 伝票番号 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <!-- クレジット伝票番号 -->
          <Condition property="creditType">
            <ne value="UnionPay" property="">
              <Lable property="recptslip" leftMargin="0"></Lable>
              <Item property="reSlipNo" leftMargin="max"></Item>
            </ne>
          </Condition>
          <!-- 伝票番号 -->
          <Condition property="creditType">
            <eq value="UnionPay" property="">
              <Lable property="invoice" leftMargin="0"></Lable>
              <Item property="reSlipNo" leftMargin="max"></Item>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
        <!-- 支払区分 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Condition property="creditType">
            <ne value="UnionPay" property="">
              <Lable property="recpttender" leftMargin="0"></Lable>
              <Item property="paymentMethod" leftMargin="max"></Item>
            </ne>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- 支払回数 -->
    <Line size="0" align="left" feedNum="0">
     <Condition property="PaymentMethodCode">
      <eq value="31" property="">
       <Lable property="recptndividedstr" leftMargin="0"></Lable>
       <Item property="NDivided" leftMargin="max"></Item>
      </eq>
     </Condition>
     <Condition property="PaymentMethodCode">
      <eq value="32" property="">
       <Lable property="recptndividedstr" leftMargin="0"></Lable>
       <Item property="NDivided" leftMargin="max"></Item>
      </eq>
     </Condition>
     <Condition property="PaymentMethodCode">
      <eq value="33" property="">
       <Lable property="recptndividedstr" leftMargin="0"></Lable>
       <Item property="NDivided" leftMargin="max"></Item>
      </eq>
     </Condition>
     <Condition property="PaymentMethodCode">
      <eq value="34" property="">
       <Lable property="recptndividedstr" leftMargin="0"></Lable>
       <Item property="NDivided" leftMargin="max"></Item>
      </eq>
     </Condition>
     <Condition property="PaymentMethodCode">
      <eq value="61" property="">
       <Lable property="recptndividedstr" leftMargin="0"></Lable>
       <Item property="NDivided" leftMargin="max"></Item>
      </eq>
     </Condition>
    </Line>
    <!-- 金 額 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Lable property="recptpaymentamount" leftMargin="0"></Lable>
          <Price property="creditPament" isMinus="false" leftMargin="max"
            unitFlg="true"></Price>
        </ne>
      </Condition>
    </Line>
    <!-- 承認番号 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Lable property="recptapproval" leftMargin="0"></Lable>
          <Item property="approvalNo" leftMargin="max"></Item>
        </ne>
      </Condition>
    </Line>
    <!-- ICカードAID -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Condition property="creditType">
            <eq value="IC" property="">
              <Lable property="icAID" leftMargin="0"></Lable>
              <Item property="icAID" leftMargin="max"></Item>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- ICカードATC -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Condition property="creditType">
            <eq value="IC" property="">
              <Lable property="icATC" leftMargin="0"></Lable>
              <Item property="icATC" leftMargin="max"></Item>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- 銀聯番号 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Condition property="creditType">
            <eq value="UnionPay" property="">
              <Lable property="unionpayNo" leftMargin="0"></Lable>
              <Item property="unionPayNo" leftMargin="max"></Item>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- 処理通番 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Condition property="creditType">
            <eq value="UnionPay" property="">
              <Lable property="treatment" leftMargin="0"></Lable>
              <Item property="issueSequence" leftMargin="max"></Item>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- 銀聯送信日時 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Condition property="creditType">
            <eq value="UnionPay" property="">
              <Lable property="unionpaypayTime" leftMargin="0"></Lable>
              <Condition property="creditIssueDateTime">
                <ne value="" property="">
                  <Date property="creditIssueDateTime" formatFrom="yy/MM/dd HH:mm:ss"
                    formatTo="MM/dd HH:mm:ss" isSysDate="false" leftMargin="max"></Date>
                </ne>
              </Condition>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- 端末識別番号 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Lable property="recptcreditterid" leftMargin="0"></Lable>
          <Item property="creditTerminalID" leftMargin="max"></Item>
        </ne>
      </Condition>
    </Line>
     <!-- 署名欄:PosLog.xmlの<Authorization SignatureRequiredFlag="true">で決めます -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Lable property="recptsignspace" leftMargin="0"></Lable>
        </ne>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
      <Condition property="signatureFlag">
        <eq value="true" property="">
          <Sign leftMargin="1" length="38" value="_"></Sign>
        </eq>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
      <Condition property="signatureFlag">
        <eq value="true" property="">
          <Sign leftMargin="0" length="1" value="|"></Sign>
          <Sign leftMargin="0" length="38" value=" "></Sign>
          <Sign leftMargin="max" length="1" value="|"></Sign>
        </eq>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
      <Condition property="signatureFlag">
        <eq value="true" property="">
          <Sign leftMargin="0" length="1" value="|"></Sign>
          <Sign leftMargin="0" length="38" value=" "></Sign>
          <Sign leftMargin="max" length="1" value="|"></Sign>
        </eq>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
      <Condition property="signatureFlag">
        <eq value="true" property="">
          <Sign leftMargin="0" length="1" value="|"></Sign>
          <Sign leftMargin="0" length="38" value="_"></Sign>
          <Sign leftMargin="max" length="1" value="|"></Sign>
        </eq>
      </Condition>
    </Line>
    <!-- 署名省略 -->
    <Line size="1" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Condition property="signatureFlag">
            <ne value="true" property="">
              <Lable property="recptsigndisplay3" leftMargin="3"></Lable>
            </ne>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Sign leftMargin="0" length="20" value="・"></Sign>
        </ne>
      </Condition>
    </Line>
    <!-- -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Sign leftMargin="0" length="1" value=" "></Sign>
        </ne>
      </Condition>
    </Line>
    <!-- 返金額 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="AdvanceAmount">
        <ne value="0">
          <Lable property="recptReturnAmount" leftMargin="2"></Lable>
          <Price property="AdvanceAmount" isMinus="false" leftMargin="max"></Price>
        </ne>
      </Condition>
    </Line>
    <!-- -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="AdvanceVoidType">
        <eq value="AdvanceVoidType">
          <Condition property="inventoryReservationID">
            <ne value="" property="">
              <Sign leftMargin="0" length="1" value=" "></Sign>
            </ne>
          </Condition>
        </eq>
      </Condition>
    </Line>
    <!-- 前受金ID -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="AdvanceVoidType">
        <eq value="AdvanceVoidType">
          <Condition property="inventoryReservationID">
            <ne value="" property="">
              <Lable property="advancePaymentIdstr" leftMargin="0"></Lable>
              <Condition property="inventoryReservationID">
                <ne value="0000000000000" property="">
                  <Item property="inventoryReservationID"
                    leftMargin="1"></Item>
                </ne>
              </Condition>
            </ne>
          </Condition>
        </eq>
      </Condition>
    </Line>
    <!-- HOLD ID、 予約ID、 客注ID -->
    <Line size="0" align="left" feedNum="0">
      <!-- HOLD ID -->
      <Condition property="HoldVoidType">
        <eq value="HoldVoid" property="">
          <Lable property="recptHoldID" leftMargin="0"></Lable>
          <Sign leftMargin="0" length="1" value=":"></Sign>
          <Item property="inventoryReservationID" leftMargin="1"></Item>
        </eq>
      </Condition>
      <!-- 客注 ID -->
      <Condition property="CustomrOrderVoidType">
        <eq value="CustomerOrderVoid" property="">
          <Lable property="recptCustomerOrderID" leftMargin="0"></Lable>
          <Sign leftMargin="0" length="1" value=":"></Sign>
          <Item property="inventoryReservationID" leftMargin="1"></Item>
        </eq>
      </Condition>
      <Condition property="rainCheckReasonCode">
        <eq value="" property="">
          <!-- 会員情報があるの時、改行する -->
          <Condition property="customerId">
            <ne value="" property="">
              <Sign leftMargin="0" length="1" value=" "></Sign>
            </ne>
          </Condition>
        </eq>
      </Condition>
    </Line>
    <!-- 会員情報 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="customerId">
        <ne value="" property="">
          <Lable property="recptpointmemberno2" leftMargin="0"></Lable>
          <Item property="customerId" leftMargin="0"></Item>
          <Condition property="customerPoints">
            <ne value="" property="">
              <Price property="customerPoints" isMinus="false"
                leftMargin="max-1"></Price>
            </ne>
          </Condition>
          <Condition property="customerPoints">
            <eq value="" property="">
              <Sign leftMargin="max-1" length="1" value="0"></Sign>
            </eq>
          </Condition>
          <Sign leftMargin="0" length="1" value="B"></Sign>
