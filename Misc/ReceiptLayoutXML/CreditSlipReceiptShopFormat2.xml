<?xml version="1.0" encoding="UTF-8" ?>
<Receipt type="Normal">
  <ReceiptPaperWidth>40</ReceiptPaperWidth>
  <ReceiptLanguage>jp</ReceiptLanguage>
  <Form>
    <!-- 再発行 -->
    <Line size="1" align="center" feedNum="0">
      <Condition property="retryflag">
        <eq value="true" property="">
          <Lable property="recptreissue" leftMargin="0"></Lable>
          <Sign length="1" leftMargin="0" value="\n"></Sign>
          <Lable property="recptnotareceipt" leftMargin="0"></Lable>
          <Sign length="1" leftMargin="0" value="\n"></Sign>
          <Lable property="recptalreadyissued" leftMargin="0"></Lable>
        </eq>
      </Condition>
    </Line>
    <!-- logo -->
    <LineImageBmp name="logo"></LineImageBmp>
    <!-- 店舖(控) -->
    <Line size="1" align="center" feedNum="1">
      <Lable property="storeRemain" leftMargin="0"></Lable>
    </Line>
    <!-- 店の資料：店名と電話番号とポスター -->
    <!-- 店名 -->
    <Line size="0" align="left" feedNum="0">
      <Lable property="recptstorenamestr" leftMargin="10"></Lable>
      <Item property="storeName" leftMargin="0"></Item>
    </Line>
    <!-- 電話番号 -->
    <Line size="0" align="left" feedNum="1">
      <Lable property="recpttelno" leftMargin="10"></Lable>
      <Item property="telNo" leftMargin="0"></Item>
    </Line>
    <!-- 担当者： -->
    <Line size="0" align="left" feedNum="1">
      <Lable property="recptoperator2" leftMargin="0"></Lable>
      <Sign length="1" leftMargin="0" value="："></Sign>
      <Item property="operatorID" leftMargin="0"></Item>
    </Line>
    <!-- イベント 名 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="barPromotionID">
        <ne value="">
          <Lable property="recptActivityHalf" leftMargin="0"></Lable>
          <Sign value=":" length="1" leftMargin="0"></Sign>
          <Item property="barPromotionName" leftMargin="0"></Item>
        </ne>
      </Condition>
    </Line>
    <!-- イベント ID -->
    <Line size="0" align="left" feedNum="1">
      <Condition property="barPromotionID">
        <ne value="">
          <Item property="barPromotionID" leftMargin="0"></Item>
        </ne>
      </Condition>
    </Line>
    <!-- train mode -->
    <Line size="0" align="left" feedNum="1">
         <Condition property="trainModeFlag">
          <eq value="True" property="">
           <Sign length="5" leftMargin="0" value="* "></Sign>
           <Sign length="1" leftMargin="0" value="*"></Sign>
           <Lable property="receiptTrainMode" leftMargin="2"></Lable>
           <Sign length="5" leftMargin="2" value="* "></Sign>
           <Sign length="1" leftMargin="0" value="*"></Sign>
          </eq>
         </Condition>
    </Line>
    <!--  社員販売 -->
    <Line size="0" align="left" feedNum="1">
      <Condition property = "BarEmployeeFlag">
       <eq value="True" property="">
        <Sign length="7" leftMargin="0" value="* "></Sign>
        <Sign length="1" leftMargin="0" value="*"></Sign>
        <Lable property="employeesales" leftMargin="1"></Lable>
        <Sign length="7" leftMargin="1" value="* "></Sign>
        <Sign length="1" leftMargin="0" value="*"></Sign>
       </eq>
      </Condition>
    </Line>
    <!-- HOLD 予約 客注 -->
    <Line size="0" align="left" feedNum="1">
      <Condition property="rainCheckReasonCode">
        <ne value="" property="">
          <!-- HOLD -->
          <Condition property="rainCheckReasonCode">
            <eq value="Hold" property="">
              <Sign length="7" leftMargin="0" value="* "></Sign>
              <Sign length="1" leftMargin="0" value="*"></Sign>
              <Lable property="recptHold" leftMargin="3"></Lable>
              <Sign length="7" leftMargin="3" value="* "></Sign>
              <Sign length="1" leftMargin="0" value="*"></Sign>
            </eq>
          </Condition>
          <!-- 予約 -->
          <Condition property="rainCheckReasonCode">
            <eq value="Reservation" property="">
              <Sign length="7" leftMargin="0" value="* "></Sign>
              <Sign length="1" leftMargin="0" value="*"></Sign>
              <Lable property="recptReservation" leftMargin="2"></Lable>
              <Sign length="7" leftMargin="2" value="* "></Sign>
              <Sign length="1" leftMargin="0" value="*"></Sign>
            </eq>
          </Condition>
          <!-- 客注 -->
          <Condition property="rainCheckReasonCode">
            <eq value="CustomerOrder" property="">
              <Sign length="7" leftMargin="0" value="* "></Sign>
              <Sign length="1" leftMargin="0" value="*"></Sign>
              <Lable property="recptCustomerOrder" leftMargin="2"></Lable>
              <Sign length="7" leftMargin="2" value="* "></Sign>
              <Sign length="1" leftMargin="0" value="*"></Sign>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- 免税 -->
    <Line size="0" align="left" feedNum="1">
      <Condition property="exemptTaxAmount">
        <ne value="0" property="">
          <Sign length="7" leftMargin="0" value="* "></Sign>
          <Sign length="1" leftMargin="0" value="*"></Sign>
          <Lable property="taxExemption" leftMargin="2"></Lable>
          <Sign length="7" leftMargin="2" value="* "></Sign>
          <Sign length="1" leftMargin="0" value="*"></Sign>
        </ne>
      </Condition>
    </Line>
    <!-- 詳細商品の資料 -->
    <LineItems>
      <Line size="0" align="left" feedNum="0">
        <!-- 商品の名 -->
        <Item property="itemName" leftMargin="0"></Item>
        <Condition property="nonSalesFlag">
          <eq value="True" property="">
            <Sign leftMargin="max" length="1" value="※"></Sign><!-- ※の意味は非売品 -->
          </eq>
        </Condition>
        <Condition property="discountableFlag">
          <eq value="False" property="">
            <Sign leftMargin="max" length="1" value="*"></Sign><!-- *の意味は非割引-->
          </eq>
        </Condition>
      </Line>
      <Line size="0" align="left" feedNum="0">
        <Condition property="itemID">
          <ne value="" property="">
            <!-- 言語は英語の時の書式： itemID × quantity -->
            <Condition property="language">
              <eq value="en" property="">
                <!-- S -->
                <Condition property="itemMark">
                  <eq value="True" property="">
                    <Sign leftMargin="0" length="1" value="S"></Sign>
                  </eq>
                </Condition>
                <Condition property="itemMark">
                  <ne value="True" property="">
                    <Sign leftMargin="0" length="1" value=" "></Sign>
                  </ne>
                </Condition>
                <Item property="itemID" leftMargin="0"></Item>
                <Condition property="IsAdvancedFlag">
        			<ne value= "true" property="">
          				<Condition property="itemNote">
                  			<eq value="" property="">
                    			<Sign value=" " length="1" leftMargin="7"></Sign>
                  			</eq>
                		</Condition>
                		<Condition property="itemNote">
                  			<ne value="" property="">
                    			<Item property="itemNote" leftMargin="1"></Item>
                  			</ne>
                		</Condition>
        			</ne>
      			</Condition>
      			<Condition property="IsAdvancedFlag">
      				<eq value= "true" property="">
      					<Sign value=" " length="1" leftMargin="7"></Sign>
      				</eq>
      			</Condition>
                <Sign leftMargin="4" length="1" value="×"></Sign>
                <Price property="quantity" isMinus="false" leftMargin="1"></Price>
                <Lable property="recptunitstr" leftMargin="0"></Lable>
              </eq>
            </Condition>
            <!-- 言語は日本語の時の書式： itemID quantity点 -->
            <Condition property="language">
              <ne value="en" property="">
                <!-- S -->
                <Condition property="itemMark">
                  <eq value="True" property="">
                    <Sign leftMargin="0" length="1" value="S"></Sign>
                  </eq>
                </Condition>
                <Condition property="itemMark">
                  <ne value="True" property="">
                    <Sign leftMargin="0" length="1" value=" "></Sign>
                  </ne>
                </Condition>
                <Item property="itemID" leftMargin="0" fixedLength="13" 
                      appendSign=" " appendWay="right"></Item>
                <Condition property="IsAdvancedFlag">
        			<ne value= "true" property="">
          				<Condition property="itemNote">
                  			<eq value="" property="">
                    			<Sign value=" " length="1" leftMargin="7"></Sign>
                  			</eq>
                		</Condition>
                		<Condition property="itemNote">
                  			<ne value="" property="">
                    			<Item property="itemNote" leftMargin="1" fixedLength="7" 
                      				appendSign=" " appendWay="right"></Item>
                  			</ne>
                		</Condition>
        			</ne>
      			</Condition>
      			<Condition property="IsAdvancedFlag">
      				<eq value= "true" property="">
      					<Sign value=" " length="1" leftMargin="7"></Sign>
      				</eq>
      			</Condition>
                <Price property="quantity" isMinus="false" leftMargin="0" fixedLength="5" 
                      				appendSign=" " appendWay="left"></Price>
                <Lable property="recptunitstr" leftMargin="0"></Lable>
              </ne>
            </Condition>
            <!-- Actual price of this item × quantity -->
            <Price property="extendedAmount" isMinus="false" leftMargin="max"></Price>
          </ne>
        </Condition>
      </Line>
      <!-- 商品の割り引き -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="discountAmount">
          <ne value="0" property="">
             <!-- ％で割引 -->
             <Condition property="priceItemDerivationRuleID">
               <eq value="1">
                  <Price property="percent" isMinus="false" leftMargin="1"></Price>
                  <Sign leftMargin="0" length="1" value="%"></Sign>
                  <Lable property="recptDiscount" leftMargin="0"></Lable>
               </eq>
             </Condition>
             <!-- 値で割引 -->
             <Condition property="priceItemDerivationRuleID">
                <eq value="2" property="">
                   <Lable property="recptitemdiscount" leftMargin="1"></Lable>
                </eq>
             </Condition>
             <!-- 手動値割引 -->
             <Condition property="priceItemDerivationRuleID">
                <eq value="3" property="">
                   <Lable property="recptPriceChange" leftMargin="1"></Lable>
                </eq>
             </Condition>
             <!-- 社員割引 -->
             <Condition property="priceItemDerivationRuleID">
                <eq value="9">
                  <Lable property="employeesales" leftMargin="1"></Lable>
                </eq>
             </Condition>
             <!-- イベント -->
             <Condition property="priceItemDerivationRuleID">
                <eq value="10">
                  <Lable property="recptActivityAll" leftMargin="1"></Lable>
                </eq>
             </Condition>
             <Price property="discountAmount" isMinus="true" leftMargin="max"></Price>
          </ne>
        </Condition>
      </Line>
       <!-- 免税額 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="exemptTaxAmount">
         <ne value="0" property="">
         <Lable property="taxExemptionStr" leftMargin="1"></Lable>
         <Price property="exemptTaxAmount" isMinus="true"
            leftMargin="max"></Price>
         </ne>
        </Condition>
      </Line>
      <Line size="0" align="left" feedNum="0">
        <Condition property="itemID">
          <ne value="" property="">
            <Sign length="1" leftMargin="0" value=" "></Sign>
          </ne>
        </Condition>
      </Line>
    </LineItems>
    <!-- 値引前合計 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="totalDiscount">
        <ne value="0">
          <Lable property="itemNoneDiscountTotal" leftMargin="5"></Lable>
          <Price property="Subtotal" isMinus="false"
            leftMargin="max"></Price>
        </ne>
      </Condition>
    </Line>
    <!-- 小計%割引 --><!-- 値引前合計 -->
    <Line size="0" align="left" feedNum="1">
      <Condition property="totalDiscount">
        <ne value="0">
          <Condition property="totalPercent">
            <ne value="0">
              <Lable property="itemAllPercentAmount" leftMargin="5"></Lable>
              <Item property="totalPercent" leftMargin="0"></Item>
              <Sign leftMargin="0" length="1" value="%"></Sign>
            </ne>
          </Condition>
          <Condition property="totalPercent">
            <eq value="0">
              <Lable property="itemAllDiscountAmount" leftMargin="5"></Lable>
            </eq>
          </Condition>
          <Price property="totalDiscount" isMinus="false"
            leftMargin="max"></Price>
        </ne>
      </Condition>
    </Line>
    <!-- 点数 -->
    <Line size="0" align="left" feedNum="0">
      <Lable property="recptnumofitems" leftMargin="2"></Lable>
      <Condition property="language">
        <eq value="en" property="">
          <Item property="allQuantity" leftMargin="max"></Item>
        </eq>
      </Condition>
      <Condition property="language">
        <ne value="en" property="">
          <Item property="allQuantity" leftMargin="max-2"></Item>
        </ne>
      </Condition>
      <Lable property="recptunitstr" leftMargin="0"></Lable>
    </Line>
    <!-- 合計 -->
    <Line size="1" align="left" feedNum="0">
      <Lable property="recpttotalstr" leftMargin="2"></Lable>
      <Price property="grandAmount" isMinus="false" leftMargin="max"></Price>
    </Line>
    <!-- 内消費税 -->
    <Line size="0" align="left" feedNum="1">
      <Condition property="exemptTaxAmount">
        <eq value="0" property="">
          <Sign leftMargin="2" length="1" value="("></Sign>
          <Lable property="recpttaxstr2" leftMargin="0"></Lable>
          <Price property="taxAmount" isMinus="false" leftMargin="max-1"></Price>
          <Sign leftMargin="0" length="1" value=")"></Sign>
        </eq>
      </Condition>
    </Line>
    <!-- -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="exemptTaxAmount">
        <ne value="0" property="">
          <Sign leftMargin="0" length="1" value=" "></Sign>
        </ne>
      </Condition>
    </Line>

    <LineItems>
      <!-- 前受金精算 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="paymentReservationID">
          <ne value="" property="">
            <Lable property="advancePaymentstr" leftMargin="4"></Lable>
            <Item property="paymentReservationID" leftMargin="1"></Item>
            <Price property="paymentAmount" isMinus="false" leftMargin="max"></Price>
          </ne>
        </Condition>
      </Line>
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherFaceValueAmount">
          <ne value="0" property="">
            <Condition property="voucherTypeCode">
              <eq value="voucher" property="">
                <!-- 金券 -->
                <Lable property="voucherStr" leftMargin="4"></Lable>
                <Price property="voucherFaceValueAmount" isMinus="false"
                  leftMargin="max"></Price>
              </eq>
            </Condition>
            <Condition property="voucherTypeCode">
              <eq value="giftcertificate" property="">
                <!-- 商品券 -->
                <Lable property="gtftCertStr" leftMargin="4"></Lable>
                <Price property="voucherFaceValueAmount" isMinus="false"
                  leftMargin="max"></Price>
              </eq>
            </Condition>
          </ne>
        </Condition>
      </Line>
    </LineItems>

    <!-- 前回前受金 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="previousAmount">
        <ne value="0" property="">
          <Lable property="previousAmountstr" leftMargin="4"></Lable>
          <Price property="previousAmount" isMinus="false" leftMargin="max"></Price>
        </ne>
      </Condition>
    </Line>
    <!-- 振込 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="bankTransferAmount">
        <ne value="0" property="">
          <Lable property="recptBankTransfer" leftMargin="4"></Lable>
          <Price property="bankTransferAmount" isMinus="false" leftMargin="max"></Price>
        </ne>
      </Condition>
    </Line>
    <!-- 現金 -->
    <Line size="0" align="left" feedNum="1">
      <Condition property="cashPament">
        <ne value="0" property="">
          <Lable property="recptcash" leftMargin="4"></Lable>
          <Price property="cashPament" isMinus="false" leftMargin="max"></Price>
        </ne>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
      <Condition property="cashPament">
        <eq value="0" property="">
          <Sign leftMargin="0" length="1" value=" "></Sign>
        </eq>
      </Condition>
    </Line>
    <!-- GiftCard -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="giftCardCount">
        <ne value="0" property="">
          <Sign leftMargin="0" length="20" value="・"></Sign>
        </ne>
      </Condition>
    </Line>
    <!-- ぎフトカードご利用 -->
    <Line size="0" align="center" feedNum="0">
      <Condition property="giftCardCount">
        <ne value="0" property="">
          <Lable property="giftCardTitleStr" leftMargin="0"></Lable>
        </ne>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
      <Condition property="giftCardCount">
        <ne value="0" property="">
          <Sign leftMargin="0" length="20" value="・"></Sign>
        </ne>
      </Condition>
    </Line>
    <LineItems>
      <!-- カード番号 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Lable property="giftCardNoStr" leftMargin="0"></Lable>
            <Item property="voucherSerialNumberEncrypt" leftMargin="max"></Item>
          </eq>
        </Condition>
      </Line>
      <!-- ご利用前金額 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Lable property="giftCardPreBalanceStr" leftMargin="0"></Lable>
            <Condition property="language">
              <eq value="en" property="">
                <Price property="voucherFaceValueAmount" isMinus="false"
                  leftMargin="max"></Price>
              </eq>
            </Condition>
            <Condition property="language">
              <ne value="en" property="">
                <Price property="voucherFaceValueAmount" isMinus="false"
                  leftMargin="max" unitFlg="true"></Price>
              </ne>
            </Condition>
          </eq>
        </Condition>
      </Line>
      <!-- ご利用金額 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Lable property="giftCardUsedStr" leftMargin="0"></Lable>
            <Condition property="language">
              <eq value="en" property="">
                <Price property="voucherAmount" isMinus="false"
                  leftMargin="max"></Price>
              </eq>
            </Condition>
            <Condition property="language">
              <ne value="en" property="">
                <Price property="voucherAmount" isMinus="false"
                  leftMargin="max" unitFlg="true"></Price>
              </ne>
            </Condition>
          </eq>
        </Condition>
      </Line>
      <!-- ご利用後金額 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Lable property="giftCardBalanceStr" leftMargin="0"></Lable>
            <Condition property="language">
              <eq value="en" property="">
                <Price property="voucherUnspentAmount" isMinus="false"
                  leftMargin="max"></Price>
              </eq>
            </Condition>
            <Condition property="language">
              <ne value="en" property="">
                <Price property="voucherUnspentAmount" isMinus="false"
                  leftMargin="max" unitFlg="true"></Price>
              </ne>
            </Condition>
          </eq>
        </Condition>
      </Line>
      <!-- 有効期限 -->
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Lable property="giftCardExpiryStr" leftMargin="0"></Lable>
            <Condition property="voucherExpirationDate">
              <ne value="">
                <Date property="voucherExpirationDate" formatFrom="yyyy-MM-dd"
                  formatTo="yyyy/MM/dd" isSysDate="false" leftMargin="max"></Date>
              </ne>
            </Condition>
          </eq>
        </Condition>
      </Line>
      <Line size="0" align="left" feedNum="0">
        <Condition property="voucherTypeCode">
          <eq value="giftcard" property="">
            <Sign leftMargin="0" length="20" value="・"></Sign>
          </eq>
        </Condition>
      </Line>
    </LineItems>
    <!-- クレジットカード -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Sign leftMargin="0" length="20" value="・"></Sign>
        </ne>
      </Condition>
    </Line>
    <Line size="0" align="center" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <!-- ICクレジットご利用 -->
          <Condition property="creditType">
            <eq value="IC" property="">
              <Lable property="icCreditTitle" leftMargin="0"></Lable>
            </eq>
          </Condition>
          <!-- 中国銀聯ご利用 -->
          <Condition property="creditType">
            <eq value="UnionPay" property="">
              <Lable property="unionpayTitle" leftMargin="0"></Lable>
            </eq>
          </Condition>
          <!-- MSクレジットご利用 -->
          <Condition property="creditType">
            <eq value="Credit" property="">
              <Lable property="recptcredittitle2" leftMargin="0"></Lable>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Sign leftMargin="0" length="20" value="・"></Sign>
        </ne>
      </Condition>
    </Line>
    <!-- カード会社 或いは 金融機関 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <!-- カード会社 -->
          <Condition property="creditType">
            <ne value="UnionPay" property="">
              <Lable property="recptcordcompanyname" leftMargin="0"></Lable>
              <Item property="cardCompany" leftMargin="max"></Item>
            </ne>
          </Condition>
          <!-- 金融機関 -->
          <Condition property="creditType">
            <eq value="UnionPay" property="">
              <Lable property="financingInstitutio" leftMargin="0"></Lable>
              <Item property="cardCompany" leftMargin="max"></Item>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- クレジット番号 或いは カード番号 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <!-- クレジット番号 -->
          <Condition property="creditType">
            <ne value="UnionPay" property="">
              <Lable property="recptcreditno" leftMargin="0"></Lable>
              <Item property="creditNoEncrypt" leftMargin="max-3"></Item>
              <Sign leftMargin="0" length="1" value="/00"></Sign>
            </ne>
          </Condition>
          <!-- カード番号 -->
          <Condition property="creditType">
            <eq value="UnionPay" property="">
              <Lable property="unionpayCardNo" leftMargin="0"></Lable>
              <Item property="creditNoEncrypt" leftMargin="max-3"></Item>
              <Sign leftMargin="0" length="1" value="/00"></Sign>
            </eq>
          </Condition>
        </ne>
      </Condition>
    </Line>
    <!-- 有効期限 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <Lable property="recptexpiry" leftMargin="0"></Lable>
          <Sign leftMargin="max" length="1" value="XX/XX"></Sign>
        </ne>
      </Condition>
    </Line>
    <!-- クレジット伝票番号 或いは 伝票番号 -->
    <Line size="0" align="left" feedNum="0">
      <Condition property="creditPament">
        <ne value="0" property="">
          <!-- クレジット伝票番号 -->
          <Condition property="creditType">
            <ne value="UnionPay" property="">
              <Lable property="recptslip" leftMargin="0"></Lable>
              <Item property="reSlipNo" leftMargin="max"></Item>
            </ne>
